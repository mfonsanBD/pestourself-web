{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate:component": "yarn plop --plopfile generators/components-plopfile.js",
    "generate:template": "yarn plop --plopfile generators/template-plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "@styled-icons/bootstrap": "^10.34.0",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.34.0",
    "@styled-icons/fa-brands": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@styled-icons/remix-line": "^10.18.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^16.0.0",
    "graphql": "^16.4.0",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "12.1.6",
    "next-auth": "^4.3.4",
    "next-pwa": "^5.2.21",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.14",
    "polished": "^4.1.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-slick": "^0.29.0",
    "react-tabs": "^5.1.0",
    "storybook-addon-next-router": "^3.1.1",
    "styled-components": "5.3.5",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@jackfranklin/test-data-bot": "^2.0.0",
    "@next/eslint-plugin-next": "^12.1.6",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/react": "6.4.22",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.39",
    "@types/react": "^18.0.9",
    "@types/react-slick": "^0.23.4",
    "@types/react-tabs": "^5.0.5",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^12.4.1",
    "msw": "^0.39.2",
    "node-fetch": "3.2.4",
    "plop": "^3.1.0",
    "prettier": "^2.3.1",
    "setimmediate": "^1.0.5",
    "start-server-and-test": "^1.12.5",
    "typescript": "^4.3.4"
  }
}
